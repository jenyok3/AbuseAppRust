    const name = String(account?.name || "");
    const notes = String(account?.notes || "");
    const combined = `${name} ${notes}`;
    const m =
      combined.match(/\bTG\s*(\d+)\b/i) ||
      name.match(/^(\d+)$/) ||
      name.match(/^\D*(\d+)$/);
    if (!m) return null;
    const id = Number(m[1]);
    return Number.isFinite(id) ? id : null;
  };

