              </>
            </div>
          </motion.div>
        </div>
      </main>
      
      {/* Project Modal */}
      <ProjectModal
        isOpen={isModalOpen}
        onClose={() => setIsModalOpen(false)}
        onSave={handleSaveProject}
        project={editingProject}
        mode={modalMode}
      />

      {/* Custom Link Modal */}
      <CustomLinkModal
        isOpen={isCustomLinkModalOpen}
        onClose={() => setIsCustomLinkModalOpen(false)}
        onSubmit={handleCustomLinkSubmit}
      />

      <Dialog open={isHashtagModalOpen} onOpenChange={(open) => (open ? setIsHashtagModalOpen(true) : closeHashtagModal())}>
        <DialogContent className="bg-[#0a0a0a] border-white/10 text-white sm:max-w-sm">
          <DialogHeader>
            <DialogTitle>Додат� хеште�</DialogTitle>
          </DialogHeader>
          <div className="space-y-2">
            <Label className="text-xs text-muted-foreground">
              {hashtagModalAccount ? `Акаунт: ${hashtagModalAccount.name}` : "Оберіт� акаунт"}
            </Label>
            <Input
              autoFocus
              value={hashtagInput}
              onChange={(e) => setHashtagInput(e.target.value)}
              placeholder="#зробле��"
              className="bg-black/50 border-white/10"
              onKeyDown={(e) => {
                if (e.key === "Enter") {
                  e.preventDefault();
                  submitHashtagModal();
                }
              }}
            />
          </div>
          <DialogFooter className="pt-2">
            <Button type="button" variant="ghost" onClick={closeHashtagModal} className="hover:bg-white/10 hover:text-white">
              Скасуват�
            </Button>
            <Button type="button" onClick={submitHashtagModal} className="bg-primary text-white hover:bg-primary/90">
              Додат�
            </Button>
          </DialogFooter>
        </DialogContent>
      </Dialog>
    </div>
  );
}


















